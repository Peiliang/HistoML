    </histo:UnificationXref>
    <histo:UnificationXref rdf:ID="UnificationXref_Tumor_Extension_into_Renal_Sinus">
        <histo:uri rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.aipath.com/histo/HISTO_0000012</histo:uri>
    </histo:UnificationXref>
    <histo:UnificationXref rdf:ID="UnificationXref_Tumor_Extension_into_Segmental_Branches_of_Renal_Vein">
