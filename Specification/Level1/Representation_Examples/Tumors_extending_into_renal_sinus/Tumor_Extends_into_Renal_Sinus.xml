    </histo:UnificationXref>
    <histo:UnificationXref rdf:ID="UnificationXref_Extension">
        <histo:uri rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://purl.obolibrary.org/obo/NCIT_C120271</histo:uri>
    </histo:UnificationXref>
    <histo:UnificationXref rdf:ID="UnificationXref_Invasion">
        <histo:uri rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://purl.obolibrary.org/obo/NCIT_C20625</histo:uri>
    </histo:UnificationXref>
    <histo:UnificationXref rdf:ID="UnificationXref_Tumor_Vascular_Invasion">
